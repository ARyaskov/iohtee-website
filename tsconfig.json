{
  "compilerOptions": {
    "target": "es2020",
    "module": "NodeNext",
    "moduleResolution": "NodeNext",
    "lib": ["es2020", "dom"],
    "jsx": "react",
    "jsxImportSource": "@emotion/react",
    "sourceMap": true,
    "declaration": true,
    "strict": false,
    "noImplicitReturns": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,
    "experimentalDecorators": true
  }
}